{
  "files.exclude": {
    "**/bin/": true,
    "**/obj/": true,
    "**/web.config": true,
    "**/npm-debug.log*": true,
    "**/yarn.lock": true,
    "**/*.csproj.user": true,
    "**/Properties/": true,
    ".vs": true
  },
  "editor.rulers": [
    100
  ],
  "auto-run-command.rules": [
    {
      "command": "wallaby.start",
      "condition": "always"
    },
    {
      "command": "workbench.action.tasks.build",
      "condition": "always"
    }
  ]
}
